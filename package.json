{
  "name": "starter-kit",
  "version": "1.0.0",
  "description": "starter kit based on typescript, nestjs, jest and prisma",
  "main": "build/applications/main.js",
  "scripts": {
    "prisma": "prisma generate --schema ./prisma/schema.prisma",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/applications/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/JollyRojer1512/starter-kit.git"
  },
  "author": "handeye1512@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JollyRojer1512/starter-kit/issues"
  },
  "homepage": "https://github.com/JollyRojer1512/starter-kit#readme",
  "dependencies": {
    "@nestjs/common": "^10.3.5",
    "@nestjs/core": "^10.3.5",
    "@nestjs/platform-express": "^10.3.5",
    "@nestjs/swagger": "^7.3.0",
    "@prisma/client": "^5.15.1",
    "axios": "^1.6.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "jest": "^29.6.1",
    "reflect-metadata": "^0.2.1",
    "prisma": "^5.15.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.4.3"
  }
}
